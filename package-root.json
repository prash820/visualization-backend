{
  "name": "chart-app-backend",
  "version": "1.0.0",
  "description": "Chart App Production Backend - Multi-tenant AWS Infrastructure Provisioning",
  "main": "visualization-backend/dist/server.js",
  "scripts": {
    "start": "npm run start:backend",
    "start:backend": "cd visualization-backend && npm start",
    "start:terraform": "cd terraform-services && python main.py",
    "build": "cd visualization-backend && npm run build",
    "postinstall": "cd visualization-backend && npm install && cd ../terraform-services && pip install -r requirements.txt",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "dependencies": {
    "@auth/core": "^0.39.1",
    "@iconify-json/logos": "^1.2.4",
    "highlight.js": "^11.11.1",
    "marked": "^15.0.12",
    "next-auth": "^4.24.11",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/react-syntax-highlighter": "^15.5.13"
  },
  "keywords": [
    "aws",
    "terraform",
    "infrastructure",
    "multi-tenant",
    "heroku",
    "production"
  ],
  "author": "Chart App Team",
  "license": "MIT"
}
