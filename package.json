{
  "name": "visualization-backend",
  "version": "1.0.0",
  "description": "Chart App Production Backend - Multi-tenant AWS Infrastructure Provisioning",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "build": "tsc && node build.js",
    "start": "node dist/server.js",
    "start:terraform": "cd terraform-runner && python main.py",
    "heroku-postbuild": "npm rebuild bcrypt --build-from-source && node build.js && npx puppeteer browsers install chrome && cp src/utils/aws-logo-names.txt dist/utils/aws-logo-names.txt && chmod +x install-terraform.sh && ./install-terraform.sh"
  },
  "keywords": [
    "aws",
    "terraform",
    "infrastructure",
    "multi-tenant",
    "heroku",
    "production"
  ],
  "author": "Chart App Team",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@aws-sdk/client-api-gateway": "^3.835.0",
    "@aws-sdk/client-cost-explorer": "^3.835.0",
    "@aws-sdk/client-dynamodb": "^3.835.0",
    "@aws-sdk/client-lambda": "^3.835.0",
    "@aws-sdk/client-s3": "^3.835.0",
    "@aws-sdk/client-sts": "^3.835.0",
    "@mermaid-js/mermaid-cli": "^11.4.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/node-fetch": "^2.6.12",
    "@types/react-router-dom": "^5.3.3",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "fs-extra": "^11.3.0",
    "he": "^1.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.98.0",
    "puppeteer-core": "^23.11.1",
    "qs": "^6.11.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/fs-extra": "^11.0.4",
    "@types/he": "^1.2.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^18.19.115"
  },
  "engines": {
    "node": "20.x",
    "npm": ">=9.0.0"
  }
